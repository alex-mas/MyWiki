{
  "name": "mywiki",
  "version": "1.0.0rc",
  "description": "",
  "main": "dist/app/main.js",
  "build": {
    "appId": "com.myWiki.app",
    "files": [
      "dist/app/**/*",
      "package.json",
      "resources/*"
    ],
    "extraFiles": [
      "plugins",
      "resources/locales"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null,
    "win": {
      "asar": true
    }
  },
  "scripts": {
    "webpack:dev": "node_modules/.bin/webpack --config webpack/webpack.config.js --env.NODE_ENV=development --debug --watch",
    "webpack:prod": "node_modules/.bin/webpack --config webpack/webpack.config.js --env.NODE_ENV=production ",
    "start": "node_modules/.bin/concurrently \"yarn run webpack:dev\" \"electron dist/app/main.js\"",
    "build": " npm run webpack:prod && electron-builder",
    "start-nobuild": "electron dist/app/main.js",
    "test": "npm run webpack:prod && jest",
    "test-nobuild": "jest",
    "postinstall": "electron-builder install-app-deps",
    "e2e": "npm run webpack:prod && jest ./e2e",
    "unit": "npm run webpack:prod && jest ./src"
  },
  "author": "axc",
  "license": "ISC",
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "^4.7.5",
    "@types/image-size": "^0.0.29",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-color": "^2.17.3",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-syntax-highlighter": "^10.1.0",
    "@types/showdown": "^1.7.5",
    "@types/slate": "^0.44.8",
    "@types/slate-plain-serializer": "^0.6.1",
    "@types/slate-react": "^0.22.0",
    "@types/uuid": "^3.4.8",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.1",
    "electron": "13.6.6",
    "electron-builder": "^20.31.2",
    "fs-extra": "^7.0.1",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.4",
