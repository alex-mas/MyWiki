{
  "name": "mywiki",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app/main.js",
  "build": {
    "appId": "com.myWiki.app",
    "files": [
      "dist/app/**/*",
      "package.json",
      "resources/*"
    ],
    "extraFiles": [
      "plugins",
      "resources/locales"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null,
    "win": {
      "asar": true
    }
  },
  "scripts": {
    "webpack:dev": "node_modules/.bin/webpack --config webpack/webpack.config.js --env.NODE_ENV=development --debug --watch",
    "webpack:prod": "node_modules/.bin/webpack --config webpack/webpack.config.js --env.NODE_ENV=production ",
    "start": "node_modules/.bin/concurrently \"npm run webpack:dev\" \"electron dist/app/main.js\"",
    "build": " npm run webpack:prod && electron-builder",
    "start-nobuild": "electron dist/app/main.js",
    "test": "npm run webpack:prod && jest",
    "test-nobuild": "jest",
    "postinstall": "electron-builder install-app-deps",
    "e2e": "npm run webpack:prod && jest ./test/e2e",
    "unit": "npm run webpack:prod && jest ./test/unit"
  },
  "author": "axc",
  "license": "ISC",
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/image-size": "^0.0.29",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/showdown": "^1.7.5",
    "@types/slate": "^0.43.4",
    "@types/slate-plain-serializer": "^0.6.1",
    "@types/slate-react": "^0.20.1",
    "@types/uuid": "^3.4.4",
    "@types/react-color": "^2.14.0",
    "@types/react-syntax-highlighter": "^10.1.0",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.1",
    "electron": "3.0.7",
    "electron-builder": "^20.31.2",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.7",
    "sass-loader": "^7.1.0",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@axc/react-components": "^0.2.5",
    "@axc/thread-manager": "^0.1.0",
    "animate.css": "^3.7.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "image-size": "^0.6.3",
    "immutable": "^3.8.2",
    "lodash.clonedeep": "^4.5.0",
    "material-design-icons": "^3.0.1",
    "re-resizable": "^4.9.2",
    "react": "^16.4.2",
    "react-color": "^2.17.0",
    "react-dom": "^16.4.2",
    "react-markdown": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-syntax-highlighter": "^10.1.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reset-css": "^4.0.1",
    "retext": "^6.0.0",
    "retext-keywords": "^4.0.2",
    "showdown": "^1.8.6",
    "slate": "^0.44.4",
    "slate-plain-serializer": "^0.6.28",
    "slate-react": "^0.21.12",
    "uuid": "^3.3.2"
  }
}
