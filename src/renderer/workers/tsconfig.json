{
    "compilerOptions": {
        "outDir": "./dist/src/workers",
        "sourceMap": true,
        "noImplicitAny": true,
        "strict": true,
        "strictNullChecks": false,
        "lib":[
            "es2017",
            "webworker"
        ],
        "module": "commonjs",
        "target": "es2017",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    },
    "include": [
        "./**/*"
    ]
}